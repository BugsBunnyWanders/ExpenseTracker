{
  "name": "expensetracker",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "postinstall": "node ./scripts/setup-polyfills.js && node ./scripts/create-ws-resolver.js",
    "diagnose": "node ./scripts/network-diagnostics.js",
    "setup-db": "echo 'Please run the SQL in scripts/setup-supabase.js in the Supabase SQL editor'",
    "check-db": "node ./scripts/check-database.js",
    "fix-profiles": "node ./scripts/fix-missing-profiles.js",
    "clear-cache": "node ./scripts/clear-local-storage.js",
    "auto-confirm-users": "node ./scripts/apply-db-changes.js auto-confirm-users",
    "disable-email-confirm": "echo 'Running SQL to disable email confirmation' && node ./scripts/apply-db-changes.js auto-confirm-users",
    "fix-db-access": "echo 'Running SQL to fix database access issues' && node ./scripts/apply-db-changes.js setup-rls-policies",
    "test-db-access": "node ./scripts/test-db-access.js",
    "disable-rls": "echo 'Running SQL to disable RLS completely (for testing only!)' && node ./scripts/apply-db-changes.js disable-rls",
    "check-schema": "node ./scripts/check-schema.js",
    "rebuild-tables": "echo 'Please run the SQL in scripts/rebuild-tables.sql in the Supabase SQL editor to completely rebuild tables with proper schema'",
    "clean-reinstall": "expo doctor --fix && rimraf node_modules && rimraf .expo && npm cache clean --force && npm install && npm run postinstall",
    "start-clean": "expo start --clear"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.12",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.12",
    "@supabase/supabase-js": "^2.49.4",
    "eas-cli": "^16.4.0",
    "expo": "~53.0.7",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.5",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.6",
    "expo-linking": "~7.1.4",
    "expo-router": "~5.0.5",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "firebase": "^11.6.1",
    "node-fetch": "^2.6.11",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
